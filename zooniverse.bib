	Author = {{Savani}, N.~P. and {Davies}, J.~A. and {Davis}, C.~J. and {Shiota}, D. and {Rouillard}, A.~P. and {Owens}, M.~J. and {Kusano}, K. and {Bothmer}, V. and {Bamford}, S.~P. and {Lintott}, C.~J. and {Smith}, A.},
	Date-Added = {2013-08-16 19:36:46 +0000},
	Date-Modified = {2013-08-16 19:36:46 +0000},
	Doi = {10.1007/s11207-012-0041-6},
	Journal = {\solphys},
	Keywords = {Coronal mass ejections (CMEs), Heliosphere, Solar-terrestrial relations, Solar wind},
