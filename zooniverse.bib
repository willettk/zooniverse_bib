	Eprint = {1403.0308},
	Journal = {\pasj},
	Keywords = {accretion, accretion disks, novae, cataclysmic variables, stars: dwarf novae, stars: individual (GALEX J194419.33+491257.0)},
	Month = may,
	Primaryclass = {astro-ph.SR},
	Title = {{GALEX J194419.33+491257.0: An unusually active SU UMa-type dwarf nova with a very short orbital period in the Kepler data}},
