	Issn = {2057-0422},
	Journal = {British Journal for Military History},
	Number = {2},
	Title = {A Life in the Trenches? The Use of Operation War Diary and Crowdsourcing Methods to Provide an Understanding of the British Army's Day-to-Day Life on the Western Front},
	Url = {http://bjmh.org.uk/index.php/bjmh/article/view/96},
	Volume = {2},
