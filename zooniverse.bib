	Journal = {\mnras},
	Keywords = {galaxies: general, cosmology: theory},
	Month = may,
	Pages = {975-980},
	Primaryclass = {astro-ph.CO},
	Title = {{Galaxy Zoo: a correlation between the coherence of galaxy spin chirality and star formation efficiency}},
