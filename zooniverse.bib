	Issn = {0019-1035},
	Journal = {Icarus},
	Keywords = {Citizen Science},
	Month = {Jan},
	Pages = {30 - 48},
	Title = {The Moon Zoo citizen science project: Preliminary results for the Apollo 17 landing site},
	Url = {http://www.sciencedirect.com/science/article/pii/S0019103516000324},
